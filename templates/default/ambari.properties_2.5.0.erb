views.request.read.timeout.millis=10000
webapp.dir=/usr/lib/ambari-server/web

<% if node['hdp']['ambari-server']['config']['api_ssl'] == 'true' -%>
# Setup Truststore
ssl.trustStore.password=<%= node['hdp']['ambari-server']['config']['ssl_truststore_password'] %>
ssl.trustStore.path=<%= node['hdp']['ambari-server']['config']['ssl_truststore_path'] %>
ssl.trustStore.type=<%= node['hdp']['ambari-server']['config']['ssl_truststore_type'] %>

# Enable HTTPS for Ambari server
api.ssl=<%= node['hdp']['ambari-server']['config']['api_ssl'] %>
client.api.ssl.cert_name=<%= node['hdp']['ambari-server']['config']['api_ssl_cert_name'] %>
client.api.ssl.key_name=<%= node['hdp']['ambari-server']['config']['api_ssl_key_name'] %>
client.api.ssl.port=<%= node['hdp']['ambari-server']['config']['api_ssl_port'] %>

# Enable Two-way SSL
security.server.two_way_ssl=<%= node['hdp']['ambari-server']['config']['two_way_ssl'] %>
<% end -%>

<% if node['hdp']['ambari-server']['config']['ldap_is_configured'] == 'true' -%>
# LDAP integration
ambari.ldap.isConfigured=<%= node['hdp']['ambari-server']['config']['ldap_is_configured'] %>
authentication.ldap.baseDn=<%= node['hdp']['ambari-server']['config']['ldap_base_dn'] %>
authentication.ldap.bindAnonymously=<%= node['hdp']['ambari-server']['config']['ldap_bind_anonymously'] %>
authentication.ldap.dnAttribute=<%= node['hdp']['ambari-server']['config']['ldap_dn_attribute'] %>
authentication.ldap.groupMembershipAttr=<%= node['hdp']['ambari-server']['config']['ldap_group_membership_attr'] %>
authentication.ldap.groupNamingAttr=<%= node['hdp']['ambari-server']['config']['ldap_group_naming_attr'] %>
authentication.ldap.groupObjectClass=<%= node['hdp']['ambari-server']['config']['ldap_group_object_class'] %>
authentication.ldap.managerDn=<%= node['hdp']['ambari-server']['config']['ldap_manager_dn'] %>
authentication.ldap.managerPassword=<%= node['hdp']['ambari-server']['config']['ldap_manager_password'] %>
authentication.ldap.primaryUrl=<%= node['hdp']['ambari-server']['config']['ldap_primary_url'] %>
authentication.ldap.referral=<%= node['hdp']['ambari-server']['config']['ldap_referral'] %>
authentication.ldap.useSSL=<%= node['hdp']['ambari-server']['config']['ldap_use_ssl'] %>
authentication.ldap.userObjectClass=<%= node['hdp']['ambari-server']['config']['ldap_user_object_class'] %>
authentication.ldap.usernameAttribute=<%= node['hdp']['ambari-server']['config']['ldap_username_attribute'] %>
client.security=ldap
<% end -%>
